{
  "htmlWhitespaceSensitivity": "ignore",
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 80,
  "semi": true,
  "plugins": ["prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": ["*.yaml", "*.yml", "*.json"],
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
